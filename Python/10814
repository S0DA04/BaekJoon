import sys;
N = int(sys.stdin.readline());
Unsorted = [];
Sorted = [];
for i in range(N):
  x,y = sys.stdin.readline().split();
  x = int(x);
  y = str(y);
  Unsorted.append([x,y]);
Unsorted.sort(key = lambda x: x[0]);
Sorted = Unsorted;
for i in Sorted:
  for j in i:
    print(j, end=" ");
  print();
